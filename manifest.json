{
    "manifest_version": 2,
    "name": "BlackThroughTimeAndSpace",
    "description": "Replaces images on sites with ones of random black celebrities",
    "version": "1.0.0",
    "icons": { "128": "bwi.png" },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "bwi.png",
        "default_popup": "Popup.html",
        "default_title": "All Black Everything"
    },
    "permissions": ["activeTab"]
}